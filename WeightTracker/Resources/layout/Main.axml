            android:textSize="@dimen/header_label_text_size" />
        <EditText
            android:inputType="numberDecimal"
            android:layout_width="@dimen/edit_text_width"
            android:layout_height="@dimen/edit_text_height"
            android:id="@+id/WeightTextBox"
            android:layout_marginLeft="@dimen/edit_text_left_margin"
            android:singleLine="true"
            android:nextFocusDown="@+id/LeftArmTextBox"
            android:textSize="@dimen/edit_text_text_size"
            android:layout_gravity="bottom" />
        <TextView
            android:text="0"
            android:layout_width="@dimen/amount_changed_width"
            android:layout_height="@dimen/header_label_height"
            android:id="@+id/WeightChange"
            android:layout_marginLeft="@dimen/amount_changed_left_margin"
