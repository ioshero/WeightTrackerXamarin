        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout1"
        android:layout_marginBottom="7dp">
        <TextView
            android:text="Date"
            android:layout_width="@dimen/date_label_width"
            android:layout_height="@dimen/header_label_height"
            android:id="@+id/textView1"
            android:gravity="center_vertical|right"
            android:layout_marginTop="3dp"
            android:layout_marginBottom="3dp"
            android:textSize="@dimen/header_label_text_size"
            android:layout_gravity="center" />
        <TextView
            android:inputType="date"
            android:layout_width="@dimen/date_picker_width"
            android:layout_height="@dimen/date_picker_height"
            android:id="@+id/Date"
            android:layout_marginLeft="15dp"
            android:textSize="@dimen/header_label_text_size"
            android:text="02/14/2014"
            android:gravity="center_vertical"
            android:textColor="#ff0f72e1" />
        <TextView
            android:text="Prev"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/header_label_height"
            android:id="@+id/textView1"
            android:gravity="right"
            android:layout_marginTop="3dp"
            android:layout_marginBottom="3dp"
            android:textSize="@dimen/header_label_text_size"
            android:layout_marginLeft="50dp" />
        <TextView
            android:text="Since Beg."
            android:layout_width="wrap_content"
            android:layout_height="@dimen/header_label_height"
            android:id="@+id/textView1"
            android:gravity="right"
            android:layout_marginTop="3dp"
            android:layout_marginBottom="3dp"
            android:textSize="@dimen/header_label_text_size"
            android:layout_marginLeft="60dp" />
    </LinearLayout>
